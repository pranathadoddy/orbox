<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Orderbox</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="orderbox.io - a multipurpose order engine" />
    <meta name="keywords" content="order, goshop, booking engine, online shop" />
    <meta name="author" content="orderbox.io team" />

    <link rel="icon" href="https://cdn.orbox.id/obox/assets/img/favicon.ico" type="image/x-icon" />
    <link rel="shortcut icon" href="https://cdn.orbox.id/obox/assets/img/favicon.ico" type="image/x-icon" />

    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap" rel="stylesheet">

    <environment include="Development">
        <link rel="stylesheet" href="~/dist/app-tenant-dev.min.css" />
    </environment>
    <environment include="Staging,Production">
        <link rel="stylesheet" href="~/dist/app-tenant-prod.min.css" />
    </environment>
</head>
<body>
    @RenderBody()

    <environment include="Development">
        <script defer src="~/dist/app-tenant-dev.js" type="text/javascript"></script>
    </environment>
    <environment include="Staging,Production">
        <script defer src="~/dist/app-tenant-prod.min.js" type="text/javascript"></script>
    </environment>
</body>
</html>